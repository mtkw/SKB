<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    <navigation-rule>
        <from-view-id>/newTemplate.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>case1</from-outcome>
            <to-view-id>/moje_ubezpieczenia.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/lista_klientów.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>customerDetails</from-outcome>
            <to-view-id>/klient_szczegóły.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>accountDetails</from-outcome>
            <to-view-id>/konto_szczegóły.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>transactionsDetails</from-outcome>
            <to-view-id>/lista_transakcji.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/wybórUbezpieczenia.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>conditions</from-outcome>
            <to-view-id>/wybórWarunków.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/wybórWarunków.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>summary</from-outcome>
            <to-view-id>/podsumowanieSymulacji.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>case1</from-outcome>
            <to-view-id>/podsumowanieSymulacji.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>summary</from-outcome>
            <to-view-id>/podsumowanieSymulacji.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/podsumowanieSymulacji.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>main</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>payement</from-outcome>
            <to-view-id>/rodzajPlatnosci.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/gotowka.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>płatności</from-outcome>
            <to-view-id>/rodzajPlatnosci.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>podsumowanieCałości</from-outcome>
            <to-view-id>/podsumowanieCałości.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/lista_transakcji.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>klient_płatności</from-outcome>
            <to-view-id>/klient_płatności.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
</faces-config>
